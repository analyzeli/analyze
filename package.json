{
  "name": "analyze",
  "version": "1.0.0-alpha.0",
  "description": "Data manipulation in a browser",
  "main": "index.html",
  "scripts": {
    "build": "browserify src/main.js -t [ vueify ] -t [ babelify ] -g [ envify --NODE_ENV production ] | uglifyjs -cm > dist/bundle.js",
    "build-dev": "browserify src/main.js -t [ vueify ] -o dist/bundle.js --debug",
    "watch": "nodemon --watch src --ext vue,js,css,html --exec 'npm run build-dev'",
    "uglify": "uglifyjs dist/bundle.js > dist/bundle.min.js",
    "server": "http-server ./ -a localhost -o"
  },
  "author": "Anton Zemlyansky",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "browserify-shim": "^3.8.14",
    "envify": "^4.0.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.17.2",
    "uglify-js": "^3.3.16",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "block-stream2": "^1.1.0",
    "csv-parse": "^1.2.0",
    "csv-parser": "^1.11.0",
    "drag-and-drop-files": "0.0.1",
    "dygraphs": "^2.1.0",
    "file-saver": "^1.3.3",
    "filestream": "^4.1.3",
    "flat": "^2.0.1",
    "highcharts": "^6.1.0",
    "highcharts-vue": "^1.0.3",
    "html-escape": "^2.0.0",
    "is": "^3.2.1",
    "is-json": "^2.0.1",
    "json2csv": "^3.7.3",
    "moment": "^2.22.1",
    "object-path": "^0.11.4",
    "query-string": "^4.3.2",
    "split": "^1.0.0",
    "stream-combiner": "^0.2.2",
    "stream-filter": "^2.1.0",
    "stream-http": "^2.6.3",
    "streamcount": "^1.0.1",
    "ternary-stream": "^2.0.1",
    "through2": "^2.0.3",
    "vue": "^2.2.1",
    "vue-chartist": "^2.0.2",
    "vue-highcharts": "0.0.10",
    "vue-material": "0.7.1",
    "xml-nodes": "^0.1.5",
    "xml-objects": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/analyzeli/analyze.git"
  }
}
