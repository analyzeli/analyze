{
  "name": "infodiz-editor",
  "version": "0.0.1",
  "description": "Data manipulation in a browser",
  "main": "index.html",
  "scripts": {
    "watch": "watchify src/index.js -o dist/bundle.js -v",
    "build": "browserify src/index.js -t babelify -g [ envify --NODE_ENV production ] | uglifyjs - > dist/bundle.js -v",
    "uglify": "uglifyjs dist/bundle.js > dist/bundle.min.js"
  },
  "author": "Anton Zemlyansky",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "browserify": "^14.0.0",
    "envify": "^4.0.0",
    "watchify": "^3.9.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common",
    "jquery": "jquery/dist/jquery.js",
    "materialize": "materialize-css/bin/materialize.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "materialize": "materialize"
  },
  "dependencies": {
    "csv-parse": "^1.2.0",
    "csv-parser": "^1.11.0",
    "drag-and-drop-files": "0.0.1",
    "file-saver": "^1.3.3",
    "filestream": "^4.1.3",
    "flat": "^2.0.1",
    "html-escape": "^2.0.0",
    "is": "^3.2.1",
    "is-json": "^2.0.1",
    "json2csv": "^3.7.3",
    "object-path": "^0.11.4",
    "query-string": "^4.3.2",
    "split": "^1.0.0",
    "stream-combiner": "^0.2.2",
    "stream-filter": "^2.1.0",
    "stream-http": "^2.6.3",
    "ternary-stream": "^2.0.1",
    "through2": "^2.0.3",
    "vue": "^2.2.1",
    "vue-chartist": "^2.0.2",
    "vue-material": "^0.7.1",
    "xml-nodes": "^0.1.5",
    "xml-objects": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infodiz/editor.git"
  }
}
